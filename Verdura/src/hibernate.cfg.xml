<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<property name="hibernate.bytecode.use_reflection_optimizer">false</property>
		<property name="hibernate.c3p0.acquire_increment">1</property>
		<property name="hibernate.c3p0.idle_test_period">100</property>
		<property name="hibernate.c3p0.max_size">100</property>
		<property name="hibernate.c3p0.max_statements">1</property>
		<property name="hibernate.c3p0.min_size">10</property>
		<property name="hibernate.c3p0.timeout">100</property>
		<property name="hibernate.cache.provider_class">org.hibernate.cache.NoCacheProvider</property>
		<property name="hibernate.dialect">org.hibernate.dialect.PostgreSQLDialect</property>
		<property name="hibernate.search.autoregister_listeners">false</property>
		<mapping class="models.SecurityUser" />
		<mapping class="models.Torder" />
		<mapping class="models.TorderDetail" />
		<mapping class="models.TinputMeasureUnit" />
		<mapping class="models.TbusinessPartnerBranch" />
		<mapping class="models.Tpurchase" />
		<mapping class="models.ToutputMeasureUnit" />
		<mapping class="models.TbusinessPartner" />
		<mapping class="models.TbusinesPartnerItem" />
		<mapping class="models.TdataType" />
		<mapping class="models.TpurchaseDetail" />
		<mapping class="models.SecurityGroup" />
		<mapping class="models.Titem" />
		<mapping class="models.TorderNumber" />
		<mapping class="models.SecurityRole" />
		<mapping class="models.TbasicData" />
	</session-factory>
</hibernate-configuration>
